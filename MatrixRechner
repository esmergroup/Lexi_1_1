		lblDeterminanteDetb = new JLabel("Determinante det(B) :");
		lblDeterminanteDetb.setFont(new Font("Tahoma", Font.BOLD, 20));
		lblDeterminanteDetb.setBounds(10, 361, 300, 30);
		contentPane.add(lblDeterminanteDetb);
		
		btnRechnen = new JButton("Rechnen");
		btnRechnen.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent arg0) {
				if(grad == 1) {
					double a = Double.parseDouble(tfA11.getText());
					tfDetA.setText(Double.toString(a));
					double a1 = Double.parseDouble(tfB11.getText());
					tfDetB.setText(Double.toString(a1));
				}
				else if(grad == 2) {
					double a, b, c, d, erg;
					a = Double.parseDouble(tfA11.getText());
					b = Double.parseDouble(tfA12.getText());
					c = Double.parseDouble(tfA21.getText());
					d = Double.parseDouble(tfA22.getText());
					erg = (a*d) - (b*c);
					tfDetA.setText(Double.toString(erg));
					
					double a1, b1, c1, d1, erg1;
					a1 = Double.parseDouble(tfB11.getText());
					b1 = Double.parseDouble(tfB12.getText());
					c1 = Double.parseDouble(tfB21.getText());
					d1 = Double.parseDouble(tfB22.getText());
					erg1 = (a1*d1) - (b1*c1);
					tfDetB.setText(Double.toString(erg1));
				}
				else if(grad == 3) {
					double a, b, c, d, e, f, g, h, i, erg;
					a = Double.parseDouble(tfA11.getText());
					b = Double.parseDouble(tfA12.getText());
					c = Double.parseDouble(tfA13.getText());
					d = Double.parseDouble(tfA21.getText());
					e = Double.parseDouble(tfA22.getText());
					f = Double.parseDouble(tfA23.getText());
					g = Double.parseDouble(tfA31.getText());
					h = Double.parseDouble(tfA32.getText());
					i = Double.parseDouble(tfA33.getText());
					erg = a * ((e*i)-(h*f)) -d*((b*i) -(h*c)) +g*((b*f) - (e*c));
					tfDetA.setText(Double.toString(erg));
					
					double a1, b1, c1, d1, e1, f1, g1, h1, i1, erg1;
					a1 = Double.parseDouble(tfB11.getText());
					b1 = Double.parseDouble(tfB12.getText());
					c1 = Double.parseDouble(tfB13.getText());
					d1 = Double.parseDouble(tfB21.getText());
					e1 = Double.parseDouble(tfB22.getText());
					f1 = Double.parseDouble(tfB23.getText());
					g1 = Double.parseDouble(tfB31.getText());
					h1 = Double.parseDouble(tfB32.getText());
					i1 = Double.parseDouble(tfB33.getText());
					erg1 = a1 * ((e1*i1)-(h1*f1)) -d1*((b1*i1) -(h1*c1)) +g1*((b1*f1) - (e1*c1));
					tfDetB.setText(Double.toString(erg1));
				}
				else if(grad == 4) {
					double a, b, c, d, e, f, g, h, i, j, k, l, m, n, o, p, erg, m1, m2, m3, m4;
					a = Double.parseDouble(tfA11.getText());
					b = Double.parseDouble(tfA12.getText());
					c = Double.parseDouble(tfA13.getText());
					d = Double.parseDouble(tfA14.getText());
					e = Double.parseDouble(tfA21.getText());
					f = Double.parseDouble(tfA22.getText());
					g = Double.parseDouble(tfA23.getText());
					h = Double.parseDouble(tfA24.getText());
					i = Double.parseDouble(tfA31.getText());
					j = Double.parseDouble(tfA32.getText());
					k = Double.parseDouble(tfA33.getText());
					l = Double.parseDouble(tfA34.getText());
					m = Double.parseDouble(tfA41.getText());
					n = Double.parseDouble(tfA42.getText());
					o = Double.parseDouble(tfA43.getText());
					p = Double.parseDouble(tfA44.getText());
					m1 = f*((k*p)-(l*o)) -j*((g*p)-(o*h)) +n*((g*l)-(k*h));
					m2 = b*((k*p)-(o*l)) -j*((c*p)-(o*d)) +n*((c*l)-(k*d));
					m3 = b*((g*p)-(o*h)) -f*((c*p)-(o*d)) +n*((c*h)-(g*d));
					m4 = b*((g*l)-(k*h)) -f*((c*l)-(k*d)) +j*((c*h)-(g*d));
					erg = +a*m1 -e*m2 +i*m3 -m*m4;
					tfDetA.setText(Double.toString(erg));
					
					double a1, b1, c1, d1, e1, f1, g1, h1, i1, j1, k1, l1, mc1, n1, o1, p1, erg1, m11, m21, m31, m41;
					a1 = Double.parseDouble(tfB11.getText());
					b1 = Double.parseDouble(tfB12.getText());
					c1 = Double.parseDouble(tfB13.getText());
					d1 = Double.parseDouble(tfB14.getText());
					e1 = Double.parseDouble(tfB21.getText());
					f1 = Double.parseDouble(tfB22.getText());
					g1 = Double.parseDouble(tfB23.getText());
					h1 = Double.parseDouble(tfB24.getText());
					i1 = Double.parseDouble(tfB31.getText());
					j1 = Double.parseDouble(tfB32.getText());
					k1 = Double.parseDouble(tfB33.getText());
					l1 = Double.parseDouble(tfB34.getText());
					mc1 = Double.parseDouble(tfB41.getText());
					n1 = Double.parseDouble(tfB42.getText());
					o1 = Double.parseDouble(tfB43.getText());
					p1 = Double.parseDouble(tfB44.getText());
					m11 = f1*((k1*p1)-(l1*o1)) -j1*((g1*p1)-(o1*h1)) +n1*((g1*l1)-(k1*h1));
					m21 = b1*((k1*p1)-(o1*l1)) -j1*((c1*p1)-(o1*d1)) +n1*((c1*l1)-(k1*d1));
					m31 = b1*((g1*p1)-(o1*h1)) -f1*((c1*p1)-(o1*d1)) +n1*((c1*h1)-(g1*d1));
					m41 = b1*((g1*l1)-(k1*h1)) -f1*((c1*l1)-(k1*d1)) +j1*((c1*h1)-(g1*d1));
					erg1 = +a1*m11 -e1*m21 +i1*m31 -mc1*m41;
					tfDetB.setText(Double.toString(erg1));
				}
				else if(grad == 5) {
					double a, b, c, d, e, f, g, h, i, j, k, l, m, n, o, p, q, r, s, t, u, v, w, x, y, m1, m2, m3, m4, m5, erg;
					a = Double.parseDouble(tfA11.getText());
					b = Double.parseDouble(tfA12.getText());
					c = Double.parseDouble(tfA13.getText());
					d = Double.parseDouble(tfA14.getText());
					e = Double.parseDouble(tfA15.getText());
					f = Double.parseDouble(tfA21.getText());
					g = Double.parseDouble(tfA22.getText());
					h = Double.parseDouble(tfA23.getText());
					i = Double.parseDouble(tfA24.getText());
					j = Double.parseDouble(tfA25.getText());
					k = Double.parseDouble(tfA31.getText());
					l = Double.parseDouble(tfA32.getText());
					m = Double.parseDouble(tfA33.getText());
					n = Double.parseDouble(tfA34.getText());
					o = Double.parseDouble(tfA35.getText());
					p = Double.parseDouble(tfA41.getText());
					q = Double.parseDouble(tfA42.getText());
					r = Double.parseDouble(tfA43.getText());
					s = Double.parseDouble(tfA44.getText());
					t = Double.parseDouble(tfA45.getText());
					u = Double.parseDouble(tfA51.getText());
					v = Double.parseDouble(tfA52.getText());
					w = Double.parseDouble(tfA53.getText());
					x = Double.parseDouble(tfA54.getText());
					y = Double.parseDouble(tfA55.getText());
					m1 = g*(m*((s*y)-(x*t))-r*((n*y)-(x*o))+w*((n*t)-(s*o)))-l*(h*((s*y)-(x*t))-r*((i*y)-(x*j))+w*((i*t)-(s*j)))+q*(h*((n*y)-(x*o))-m*((i*y)-(x*j))+w*((i*o)-(n*j))-v*(h*((n*t)-(s*o))-m*((i*t)-(j*s))+r*((i*o)-(n*j))));
					m2 = b*(m*((s*y)-(x*t))-r*((n*y)-(j*s))+r*((i*o)-(n*j)))-l*(c*((n*y)-(x*o))-m*((d*y)-(x*e))+w*((d*o)-(n*e)))+q*(c*((n*y)-(x*o))-m*((d*y)-(x*e))+w*((d*o)-(n*e))-v*(c*((n*t)-(s*o))-m*((d*t)-(s*e))+r*((d*o)-(n*e))));
					m3 = b*(h*((s*y)-(x*t))-r*((i*y)-(j*x))+w*((i*t)-(s*j)))-g*(c*((s*y)-(x*t))-r*((d*y)-(x*e))+w*((d*t)-(s*e)))+q*(c*((i*y)-(j*x))-h*((d*y)-(x*e))+w*((d*j)-(i*e))-v*(c*((i*t)-(s*j))-h*((d*t)-(s*e))+r*((d*j)-(i*e))));
					m4 = b*(h*((n*y)-(x*o))-m*((i*y)-(j*x))+w*((i*o)-(n*j)))-g*(c*((n*y)-(x*o))-m*((d*y)-(x*e))+w*((d*o)-(n*e)))+l*(c*((i*y)-(x*j))-h*((d*y)-(x*e))+w*((d*j)-(i*e))-v*(c*((i*o)-(n*j))-h*((d*o)-(n*e))+m*((d*j)-(i*e))));
					m5 = b*(h*((n*t)-(s*o))-m*((i*t)-(j*s))+r*((i*o)-(n*j)))-g*(c*((n*t)-(s*o))-m*((d*t)-(s*e))+r*((d*o)-(n*e)))+l*(c*((i*t)-(j*s))-h*((d*t)-(s*e))+r*((d*j)-(i*e))-q*(c*((i*o)-(j*n))-h*((d*o)-(n*e))+m*((d*j)-(i*e))));
					erg = a*m1 -f*m2 +k*m3 -p*m4 +u*m5;
					tfDetA.setText(Double.toString(erg));
					
					double a1, b1, c1, d1, e1, f1, g1, h1, i1, j1, k1, l1, mc1, n1, o1, p1, q1, r1, s1, t1, u1, v1, w1, x1, y1, erg1, m11, m21, m31, m41, m51;
					a1 = Double.parseDouble(tfB11.getText());
					b1 = Double.parseDouble(tfB12.getText());
					c1 = Double.parseDouble(tfB13.getText());
					d1 = Double.parseDouble(tfB14.getText());
					e1 = Double.parseDouble(tfB15.getText());
					f1 = Double.parseDouble(tfB21.getText());
					g1 = Double.parseDouble(tfB22.getText());
					h1 = Double.parseDouble(tfB23.getText());
					i1 = Double.parseDouble(tfB24.getText());
					j1 = Double.parseDouble(tfB25.getText());
					k1 = Double.parseDouble(tfB31.getText());
					l1 = Double.parseDouble(tfB32.getText());
					mc1 = Double.parseDouble(tfB33.getText());
					n1 = Double.parseDouble(tfB34.getText());
					o1 = Double.parseDouble(tfB35.getText());
					p1 = Double.parseDouble(tfB41.getText());
					q1 = Double.parseDouble(tfB42.getText());
					r1 = Double.parseDouble(tfB43.getText());
					s1 = Double.parseDouble(tfB44.getText());
					t1 = Double.parseDouble(tfB45.getText());
					u1 = Double.parseDouble(tfB51.getText());
					v1 = Double.parseDouble(tfB52.getText());
					w1 = Double.parseDouble(tfB53.getText());
					x1 = Double.parseDouble(tfB54.getText());
					y1 = Double.parseDouble(tfB55.getText());
					m11 = g1*(mc1*((s1*y1)-(x1*t1))-r1*((n1*y1)-(x1*o1))+w1*((n1*t1)-(s1*o1)))-l1*(h1*((s1*y1)-(x1*t1))-r1*((i1*y1)-(x1*j1))+w1*((i1*t1)-(s1*j1)))+q1*(h1*((n1*y1)-(x1*o1))-mc1*((i1*y1)-(x1*j1))+w1*((i1*o1)-(n1*j1))-v1*(h1*((n1*t1)-(s1*o1))-mc1*((i1*t1)-(j1*s1))+r1*((i1*o1)-(n1*j1))));
					m21 = b1*(mc1*((s1*y1)-(x1*t1))-r1*((n1*y1)-(j1*s1))+r1*((i1*o1)-(n1*j1)))-l1*(c1*((n1*y1)-(x1*o1))-mc1*((d1*y1)-(x1*e1))+w1*((d1*o1)-(n1*e1)))+q1*(c1*((n1*y1)-(x1*o1))-mc1*((d1*y1)-(x1*e1))+w1*((d1*o1)-(n1*e1))-v1*(c1*((n1*t1)-(s1*o1))-mc1*((d1*t1)-(s1*e1))+r1*((d1*o1)-(n1*e1))));
					m31 = b1*(h1*((s1*y1)-(x1*t1))-r1*((i1*y1)-(j1*x1))+w1*((i1*t1)-(s1*j1)))-g1*(c1*((s1*y1)-(x1*t1))-r1*((d1*y1)-(x1*e1))+w1*((d1*t1)-(s1*e1)))+q1*(c1*((i1*y1)-(j1*x1))-h1*((d1*y1)-(x1*e1))+w1*((d1*j1)-(i1*e1))-v1*(c1*((i1*t1)-(s1*j1))-h1*((d1*t1)-(s1*e1))+r1*((d1*j1)-(i1*e1))));
					m41 = b1*(h1*((n1*y1)-(x1*o1))-mc1*((i1*y1)-(j1*x1))+w1*((i1*o1)-(n1*j1)))-g1*(c1*((n1*y1)-(x1*o1))-mc1*((d1*y1)-(x1*e1))+w1*((d1*o1)-(n1*e1)))+l1*(c1*((i1*y1)-(x1*j1))-h1*((d1*y1)-(x1*e1))+w1*((d1*j1)-(i1*e1))-v1*(c1*((i1*o1)-(n1*j1))-h1*((d1*o1)-(n1*e1))+mc1*((d1*j1)-(i1*e1))));
					m51 = b1*(h1*((n1*t1)-(s1*o1))-mc1*((i1*t1)-(j1*s1))+r1*((i1*o1)-(n1*j1)))-g1*(c1*((n1*t1)-(s1*o1))-mc1*((d1*t1)-(s1*e1))+r1*((d1*o1)-(n1*e1)))+l1*(c1*((i1*t1)-(j1*s1))-h1*((d1*t1)-(s1*e1))+r1*((d1*j1)-(i1*e1))-q1*(c1*((i1*o1)-(j1*n1))-h1*((d1*o1)-(n1*e1))+mc1*((d1*j1)-(i1*e1))));
					erg1 = a1*m11 -f1*m21 +k1*m31 -p1*m41 +u1*m51;
					tfDetB.setText(Double.toString(erg1));
				}
			}
		});
		btnRechnen.setBackground(Color.ORANGE);
		btnRechnen.setBounds(10, 407, 190, 23);
		contentPane.add(btnRechnen);
		
