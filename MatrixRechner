		btnAminusB = new JButton("A-B");
		btnAminusB.addActionListener(new ActionListener() {
			public void actionPerformed(ActionEvent arg0) {
				if(grad == 1) {
					a11 = Double.parseDouble(tfA11.getText());
					b11 = Double.parseDouble(tfB11.getText());
					e11 = a11 - b11;
					tfE11.setText(Double.toString(e11));
				}
				else if(grad == 2) {
					a11 = Double.parseDouble(tfA11.getText());
					a12 = Double.parseDouble(tfA12.getText());
					a21 = Double.parseDouble(tfA21.getText());
					a22 = Double.parseDouble(tfA22.getText());
					b11 = Double.parseDouble(tfB11.getText());
					b12 = Double.parseDouble(tfB12.getText());
					b21 = Double.parseDouble(tfB21.getText());
					b22 = Double.parseDouble(tfB22.getText());
					
					e11 = a11 - b11;
					e12 = a12 - b12;
					e21 = a21 - b21;
					e22 = a22 - b22;
					
					tfE11.setText(Double.toString(e11));
					tfE12.setText(Double.toString(e12));
					tfE21.setText(Double.toString(e21));
					tfE22.setText(Double.toString(e22));
				}
				else if(grad == 3) {
					a11 = Double.parseDouble(tfA11.getText());
					a12 = Double.parseDouble(tfA12.getText());
					a13 = Double.parseDouble(tfA13.getText());
					a21 = Double.parseDouble(tfA21.getText());
					a22 = Double.parseDouble(tfA22.getText());
					a23 = Double.parseDouble(tfA23.getText());
